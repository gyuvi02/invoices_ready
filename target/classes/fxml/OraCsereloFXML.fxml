<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="szamlak.OraCsereloController">
   <bottom>
      <HBox prefHeight="65.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="338.0">
               <children>
                  <Button fx:id="kilepo" onAction="#visszaAdatablakra" prefHeight="20.0" prefWidth="150.0" text="Mentés és visszalépés" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets left="50.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="megse" alignment="CENTER_RIGHT" onAction="#visszaAdatablakra" prefHeight="20.0" text="Mégse">
                     <VBox.margin>
                        <Insets right="50.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </bottom>

</BorderPane>
